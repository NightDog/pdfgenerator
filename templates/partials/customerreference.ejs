<div class="reference">
    <table>
    	<tr>
            <td class="label">Date d'entrée</td>
            <td><%= tenant.contract.beginDate %></td>
        </tr>
        <tr>
            <td class="label">Référence facture</td>
            <td><%= rent.billingReference %></td>
        </tr>
        <tr>
            <td class="label">Période de location</td>
            <td><%= rent.period %></td>
        </tr>
        <% if (landlord.isCompany && landlord.companyInfo.vatNumber) { %>
            <tr>
                <td class="label">Numéro de TVA intra.</td>
                <td><%= landlord.companyInfo.vatNumber %></td>
            </tr>
        <% } %>
    </table>
</div>
