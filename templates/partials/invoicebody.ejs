<tr>
  <th>Description</th>
  <th>Quantit√©</th>
  <th>Prix Unitaire H.T.</th>
  <th>Total H.T.</th>
</tr>

<% rent.preTaxAmounts.forEach(function(preTaxAmount) { %>
	<tr class="item-row">
	  <td class="description">
	  	<%= preTaxAmount.description %>
	  </td>
		<td class="txtAlignRight">1</td>
	  <td class="txtAlignRight"><%= preTaxAmount.amount.toFixed(2) %></td>
		<td class="txtAlignRight"><%= preTaxAmount.amount.toFixed(2) %></td>
	</tr>
<% }); %>

<% rent.charges.forEach(function(charge) { %>
		<tr class="item-row">
		  <td class="description">
		  	<%= charge.description %>
		  </td>
			<td class="txtAlignRight">1</td>
			<td class="txtAlignRight"><%= charge.amount.toFixed(2) %></td>
			<td class="txtAlignRight"><%= charge.amount.toFixed(2) %></td>
		</tr>
<% }); %>

<% rent.discounts.forEach(function(discount) { %>
	<tr class="item-row">
	  <td class="description">
			<%= discount.description %>
	  </td>
	  <td class="txtAlignRight">1</td>
	  <td class="txtAlignRight">- <%= discount.amount.toFixed(2) %></td>
		<td class="txtAlignRight">- <%= discount.amount.toFixed(2) %></td>
	</tr>
<% }); %>

<% rent.debts.forEach(function(debt) { %>
		<tr class="item-row">
		  <td class="description">
		  	<%= debt.description %>
		  </td>
			<td class="txtAlignRight">1</td>
			<td class="txtAlignRight"><%= debt.amount.toFixed(2) %></td>
			<td class="txtAlignRight"><%= debt.amount.toFixed(2) %></td>
		</tr>
<% }); %>

<%
	var emptyRowCount = 9 - rent.preTaxAmounts.length - rent.charges.length - rent.discounts.length - rent.debts.length;
	for (var idx=0; idx<emptyRowCount; idx++) {
%>
	<tr class="item-row">
		<td class="description">&nbsp;</td>
		<td class="txtAlignRight">&nbsp;</td>
		<td class="txtAlignRight">&nbsp;</td>
		<td class="txtAlignRight">&nbsp;</td>
	</tr>
<% } %>